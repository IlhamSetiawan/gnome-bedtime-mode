#: src/modules/ColorTone.js:57
msgid "Grayscale"
msgstr "Escala de grises"

#: src/modules/ColorTone.js:63
msgid "Amber"
msgstr "Ámbar"

#: src/modules/ColorTone.js:69
msgid "Green"
msgstr "Verde"

#: src/modules/ColorTone.js:75
msgid "Cyan"
msgstr "Cian"

#: src/modules/ColorTone.js:81
msgid "Sepia"
msgstr "Sepia"

#: src/modules/Decorator.js:131
msgid "Turn Off Bedtime Mode"
msgstr "Apagar modo descanso"

#: src/modules/Decorator.js:131
msgid "Turn On Bedtime Mode"
msgstr "Encender modo descanso"

#: src/ui/gtk3/preferences.ui:74 src/ui/gtk4/preferences.ui:59
msgid "Automatic Schedule"
msgstr "Horario automático"

#: src/ui/gtk3/preferences.ui:140 src/ui/gtk4/preferences.ui:96
msgid "Start Time"
msgstr "Hora de inicio"

#: src/ui/gtk3/preferences.ui:153 src/ui/gtk4/preferences.ui:103
msgid ""
"<small>Specifies the time when Bedtime Mode will be automatically turned on."
"</small>"
msgstr ""
"<small>Especifica la hora a la que se activará automáticamente el modo "
"descanso.</small>"

#: src/ui/gtk3/preferences.ui:279 src/ui/gtk4/preferences.ui:180
msgid "End Time"
msgstr "Hora de fin"

#: src/ui/gtk3/preferences.ui:292 src/ui/gtk4/preferences.ui:187
msgid ""
"<small>Specifies the time when Bedtime Mode will be automatically turned off."
"</small>"
msgstr ""
"<small>Especifica la hora en la que el modo descanso se apagará "
"automáticamente.</small>"

#: src/ui/gtk3/preferences.ui:410 src/ui/gtk4/preferences.ui:254
msgid "On-Demand"
msgstr "A la carta"

#: src/ui/gtk3/preferences.ui:456 src/ui/gtk4/preferences.ui:282
msgid "Show Button"
msgstr "Mostrar botón"

#: src/ui/gtk3/preferences.ui:469 src/ui/gtk4/preferences.ui:289
msgid ""
"<small>Choose to always show or hide the button or display it only when the "
"schedule is active.</small>"
msgstr ""
"<small>Muestra u oculta siempre el botón o sólo lo muestra cuando la "
"programación esté activa.</small>"

#: src/ui/gtk3/preferences.ui:500 src/ui/gtk4/preferences.ui:309
msgid "Always"
msgstr "Siempre"

#: src/ui/gtk3/preferences.ui:501 src/ui/gtk4/preferences.ui:310
msgid "On Active Schedule"
msgstr "En horario activo"

#: src/ui/gtk3/preferences.ui:502 src/ui/gtk4/preferences.ui:311
msgid "Never"
msgstr "Nunca"

#: src/ui/gtk3/preferences.ui:554 src/ui/gtk4/preferences.ui:346
msgid "Button Location"
msgstr "Ubicación del botón"

#: src/ui/gtk3/preferences.ui:567 src/ui/gtk4/preferences.ui:353
msgid ""
"<small>Add a button to Top Bar or System Menu. The button can be used to "
"manually toggle the Bedtime Mode.</small>"
msgstr ""
"<small>Añade un botón a la barra superior o al menú del sistema. El botón se "
"puede utilizar para alternar manualmente el modo descanso.</small>"

#: src/ui/gtk3/preferences.ui:598 src/ui/gtk4/preferences.ui:373
msgid "Top Bar"
msgstr "Barra superior"

#: src/ui/gtk3/preferences.ui:599 src/ui/gtk4/preferences.ui:374
msgid "System Menu"
msgstr "Menú del sistema"

#: src/ui/gtk3/preferences.ui:651 src/ui/gtk4/preferences.ui:409
msgid "Top Bar On/Off Indicator"
msgstr "Indicador de encendido/apagado de la barra superior"

#: src/ui/gtk3/preferences.ui:664 src/ui/gtk4/preferences.ui:416
msgid ""
"<small>Change the button appearance in Top Bar when Bedtime Mode is active/"
"inactive.</small>"
msgstr ""
"<small>Cambia la apariencia del botón en la barra superior cuando el modo "
"descanso está activo/inactivo.</small>"

#: src/ui/gtk3/preferences.ui:742 src/ui/gtk4/preferences.ui:466
msgid "Top Bar Manual Position"
msgstr "Posición manual en la barra superior"

#: src/ui/gtk3/preferences.ui:755 src/ui/gtk4/preferences.ui:473
msgid ""
"<small>Manually change the button position in Top Bar. Zero is the start of "
"the icons row.</small>"
msgstr ""
"<small>Cambia manualmente la posición del botón en la barra superior. El "
"cero es el inicio de la fila de iconos.</small>"

#: src/ui/gtk3/preferences.ui:856 src/ui/gtk4/preferences.ui:531
msgid "Color Preset and Apply Factor"
msgstr "Preajuste de color y factor de aplicación"

#: src/ui/gtk3/preferences.ui:903 src/ui/gtk4/preferences.ui:560
msgid "Color Preset"
msgstr "Preajuste de color"

#: src/ui/gtk3/preferences.ui:916 src/ui/gtk4/preferences.ui:567
msgid ""
"<small>Select the color preset to be used when Bedtime Mode is active.</"
"small>"
msgstr ""
"<small>Selecciona el preajuste de color que se utilizará cuando esté activo "
"el modo descanso.</small>"

#: src/ui/gtk3/preferences.ui:996 src/ui/gtk4/preferences.ui:619
msgid "Color Apply Factor"
msgstr "Factor de aplicación del color"

#: src/ui/gtk3/preferences.ui:1009 src/ui/gtk4/preferences.ui:626
msgid ""
"<small>The percentage of how much of the selected color preset is to be "
"applied.</small>"
msgstr ""
"<small>El porcentaje de la cantidad del preajuste de color seleccionado que "
"debe aplicarse.</small>"

#: src/ui/gtk3/preferences.ui:1102 src/ui/gtk4/preferences.ui:682
msgid "Start Bedtime Mode Now"
msgstr "Iniciar el modo descanso ahora"

msgid "Grayscale"
msgstr "Odstíny šedi"

#: src/modules/Presets.js:15
msgid "Amber"
msgstr "Jantarová"

#: src/modules/Presets.js:21
msgid "Green"
msgstr "Zelená"

#: src/modules/Presets.js:27
msgid "Cyan"
msgstr "Azurová"

#: src/modules/Presets.js:33
